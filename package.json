{
  "name": "react-alp-dev",
  "version": "0.3.1",
  "description": "react-alp - react components for cross-platform webapp",
  "main": "./src/*",
  "scripts": {
    "start": "npm run test && npm run lint && npm run build",
    "lint": "eslint src",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "build": "./scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TongChia/react-alp.git"
  },
  "keywords": [
    "react",
    "components",
    "webapp",
    "web",
    "app",
    "ios",
    "android",
    "windows",
    "material",
    "ui",
    "ionic"
  ],
  "author": "Tong Chia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TongChia/react-alp/issues"
  },
  "homepage": "https://github.com/TongChia/react-alp#readme",
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "underscore",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys"
    ]
  },
  "peerDependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "events": "^1.1.1",
    "google-material-color": "^1.2.6",
    "react-addons-create-fragment": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.13.0",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^14.1.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-polyfill": "^6.11.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-stage-1": "^6.13.0",
    "enzyme": "^2.4.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^9.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "jest-cli": "^14.1.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1",
    "shelljs": "^0.7.3",
    "stylus": "^0.54.5"
  }
}
