{
  "name": "react-alp-dev",
  "version": "0.1.0",
  "description": "react-alp - react components for cross-platform webapp",
  "main": "./src/*",
  "scripts": {
    "start": "npm run serve",
    "lint": "eslint src docs",
    "posttest": "npm run lint",
    "release:major": "npm version prerelease && git push --follow-tags && npm publish --tag beta",
    "release:minor": "npm version prerelease && git push --follow-tags && npm publish --tag beta",
    "release:patch": "npm version prerelease && git push --follow-tags && npm publish --tag beta",
    "serve": "webpack-dev-server --open --env dev",
    "serve:dist": "webpack-dev-server --open --env dist -p --progress",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false --reporters=mocha,coverage",
    "build": "./scripts/build.js",
    "build:docs": "rimraf dist/* && copyfiles -f ./docs/index.html ./docs/favicon.ico ./dist && webpack --progress --bail --env dist -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TongChia/react-alp.git"
  },
  "keywords": [
    "react",
    "components",
    "webapp",
    "web",
    "app",
    "ios",
    "android",
    "windows",
    "material",
    "ui",
    "ionic"
  ],
  "author": "Tong Chia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TongChia/react-alp/issues"
  },
  "homepage": "https://github.com/TongChia/react-alp#readme",
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-polyfill": "^6.11.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.11.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^9.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^1.0.0",
    "eslint-plugin-react": "^5.0.0",
    "file-loader": "^0.8.5",
    "install": "^0.8.1",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-junit-reporter": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "nib": "~1.1.0",
    "npm": "^3.10.5",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.0.1",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.6.1",
    "rimraf": "^2.5.2",
    "shelljs": "^0.7.3",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.6",
    "webpack-dev-server": "^2.0.0-beta"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react-addons-create-fragment": "^15.3.0"
  }
}
